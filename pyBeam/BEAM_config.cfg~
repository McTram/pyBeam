% Web & flange thickness [m]
W_THICKNESS = 0.001

% web height [m]
W_HEIGHT = 40*1e-2

% flange width [m]
F_WIDTH = 20*1e-2

% Number of overall nodes along the wing (no collapsed) (n. FEM elements = N_NODES -1 )
N_NODES = 101  

% Young modulus [GPa]
Y_MODULUS = 70*1e9

% Poisson Ratio
POISSON = 0.3

%  Beam Density [kg/m^3]
RHO = 2.7e3

% BEAM Length [m]
B_LENGTH = 30

% Number of rigid modes to be calculated
N_DOF = 6  

% Applied load [N]	
LOAD = 5000

% Follower load flag (NON_FOLLOWER, FOLLOWER, APPROX_FOLLOWER)
FOLLOWER_FLAG = NON_FOLLOWER		

% Number of loadsteps used in the analysis (DEFAULT = 1)
LOAD_STEPS = 1

% Number of iterations used in the analysis  (DEFAULT = 1)
N_STRUCT_ITER = 30




	
	
	
	
	
	
	
	
	
	
	
	
